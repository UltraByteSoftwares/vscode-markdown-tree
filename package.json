{
  "name": "markdown-tree",
  "displayName": "Markdown Tree",
  "description": "Generate collapsible trees in markdown",
  "version": "1.0.1",
  "icon": "./res/logo.png",
  "publisher": "UltraByteSoftwares",
  "license": "MIT",
  "keywords": [
    "tree",
    "markdown",
    "ultree",
    "collapsible",
    "expandable"
  ],
  "engines": {
    "vscode": "^1.78.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "ultree.generateFolderTreeFromSelection",
          "group": "ultree"
        }
      ]
    },
    "commands": [
      {
        "command": "ultree.generateFolderTreeFromSelection",
        "title": "Ultree: Generate Folder Tree"
      }
    ],
    "markdown.previewStyles": [
      "./style/branched.css"
    ],
    "markdown.markdownItPlugins": true
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.78.2",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "markdown-it-ultree": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UltraByteSoftwares/vscode-markdown-tree"
  }
}
